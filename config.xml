<?xml version='1.0' encoding='utf-8'?>
<widget id="xxxx" version="5.6.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>El Sur Es Vida</name>
    <description>Aplicativo El Sur Es Vida</description>
    <author email="rolanskapunkhc@hotmail.com" href="https:www.unasur.int">UNASUR</author>
    <content src="index.html" />
    <preference name="cordova-custom-config-autorestore" value="false" />
    
    
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
   
    <platform name="android">
        <preference name="android-minSdkVersion" value="16" />
        <preference name="AndroidLaunchMode" value="singleTask" />
        <preference name="android-manifest/application/activity/@android:windowSoftInputMode" value="adjustPan" />
        <hook src="hooks/001_before_build_android.sh" type="before_build" />
        <hook src="hooks/000_after_prepare_android.sh" type="after_prepare" />
       
       
    </platform>
    <plugin name="cordova-plugin-device" spec="^1.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.1.0" />
    <plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator" spec="~2.9.9" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.3" />
    <plugin name="cordova-plugin-device-name" spec="~1.0.0" />
    <engine name="cordova" version=">=6.4.0" />
    <engine name="ios" spec="~4.4.0" />
    
    
    <plugin name="cordova-plugin-appinfo" spec="~2.0.3" />
    <plugin name="cordova-plugin-dialogs" spec="1.3.1" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="2.1.0" />
    <plugin name="cordova-plugin-wkwebview-engine" spec="^1.1.0" />
    <plugin name="cordova-plugin-customurlscheme" spec="~4.2.0">
        <variable name="URL_SCHEME" value="xxx" />
        <variable name="ANDROID_SCHEME" value=" " />
        <variable name="ANDROID_HOST" value=" " />
        <variable name="ANDROID_PATHPREFIX" value="/" />
    </plugin>

    <!--plugin name="cordova.plugins.diagnostic" spec="3.6.5" /-->
    <plugin name="cordova-plugin-actionsheet" spec="2.3.3" />
</widget>
