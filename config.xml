<?xml version='1.0' encoding='utf-8'?>
<widget id="xxxx" version="5.6.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>xxx</name>
    <description>xxx</description>
    <author email="rolanskapunkhc@hotmail.com" href="https:www.unasur.int">UNASUR</author>
    <content src="index.html" />
    <preference name="cordova-custom-config-autorestore" value="false" />
    <preference name="BackgroundColor" value="0xff4793cf" />
    <preference name="SplashScreenBackgroundColor" value="0xff4793cf" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value="200" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="orientation" value="portrait" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <hook src="scripts/link_res.js" type="before_prepare" />
    <hook src="scripts/save_release_artifacts.js" type="after_build" />
    <platform name="ios">
        <config-file target="*-Info.plist" parent="CFBundleName">
          <string>xxxx</string>
        </config-file>
        <config-file target="*-Info.plist" parent="CFBundleDisplayName">
          <string>xxx</string>
        </config-file>
        <feature name="CDVWKWebViewEngine">
            <param name="ios-package" value="CDVWKWebViewEngine" />
        </feature>
        <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
        <preference name="AllowBackForwardNavigationGestures" value="true" />
        <config-file parent="ITSAppUsesNonExemptEncryption" target="*-Info.plist">
            <false />
        </config-file>
        <config-file overwrite="true" parent="UISupportedInterfaceOrientations" target="*-Info.plist">
            <array>
                <string>UIInterfaceOrientationPortrait</string>
            </array>
        </config-file>
        <config-file overwrite="true" parent="UISupportedInterfaceOrientations~ipad" target="*-Info.plist">
            <array>
                <string>UIInterfaceOrientationPortrait</string>
            </array>
        </config-file>
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#4793cf" />
        <preference name="StatusBarStyle" value="lightcontent" />
        <preference name="AllowInlineMediaPlayback" value="true"/>
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        
    </platform>
    <platform name="android">
        <preference name="android-minSdkVersion" value="16" />
        <preference name="AndroidLaunchMode" value="singleTask" />
        <preference name="android-manifest/application/activity/@android:windowSoftInputMode" value="adjustPan" />
        <hook src="hooks/001_before_build_android.sh" type="before_build" />
        <hook src="hooks/000_after_prepare_android.sh" type="after_prepare" />
       
        <preference name="SplashMaintainAspectRatio" value="true" />
        <preference name="SplashShowOnlyFirstTime" value="false" />
        <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
        <preference name="xwalkMode" value="embedded" />
        <preference name="xwalkMultipleApk" value="true" />
    </platform>
    <plugin name="cordova-plugin-device" spec="^1.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.1.0" />
    <plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator" spec="~2.9.9" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.3" />
    <plugin name="cordova-plugin-device-name" spec="~1.0.0" />
    <engine name="cordova" version=">=6.4.0" />
    <engine name="ios" spec="~4.4.0" />
    <engine name="windows" spec="~4.3.1" />
    <engine name="android" spec="~4.2.3" />
    
    <plugin name="phonegap-plugin-push" spec="1.9.4">
        <param name="SENDER_ID" value="xxxx" />
        <variable name="SENDER_ID" value="xxxx" />
    </plugin>
    
    
    <plugin name="cordova-plugin-facebook4" spec="1.7.4">
        <variable name="APP_ID" value="xxxxx" />
        <variable name="APP_NAME" value="xxx" />
    </plugin>
    
    <plugin name="cordova-plugin-x-socialsharing" spec="https://github.com/EddyVerbruggen/SocialSharing-PhoneGap-Plugin.git#5.1.1" />
    <plugin name="phonegap-plugin-barcodescanner" spec="3.1.0" />
    <plugin name="cordova-plugin-geolocation" spec="~2.2.0" />
    <plugin name="cordova-custom-config" spec="https://github.com/dpa99c/cordova-custom-config#master" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.2" />
    <plugin name="cordova-plugin-appinfo" spec="~2.0.3" />
    <plugin name="cordova-plugin-dialogs" spec="1.3.1" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="2.1.0" />
    <plugin name="cordova-plugin-wkwebview-engine" spec="^1.1.0" />
    <plugin name="cordova-plugin-customurlscheme" spec="~4.2.0">
        <variable name="URL_SCHEME" value="xxx" />
        <variable name="ANDROID_SCHEME" value=" " />
        <variable name="ANDROID_HOST" value=" " />
        <variable name="ANDROID_PATHPREFIX" value="/" />
    </plugin>
    <plugin name="cordova-plugin-vibration" spec="~2.1.1" />
    <plugin name="cordova-plugin-inappbrowser" spec="1.7.1" />
    <plugin name="cordova-plugin-appavailability" spec="0.4.2" />
    <!--plugin name="cordova.plugins.diagnostic" spec="3.6.5" /-->
    <plugin name="cordova-plugin-actionsheet" spec="2.3.3" />
</widget>
